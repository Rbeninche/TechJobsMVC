<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>


@*// TODO #3 - Use the same list display from Views/Search/Index.cshtml*@








@foreach (var dict in ViewBag.jobs)

{
<table class="job-listing">
    <tbody>
        @foreach (var key in dict.Keys)
        {




            <tr>


                <td>@key</td>
                <td>@dict[key]</td>

            </tr>





        }
    </tbody>
    </table>

 }


